<!DOCTYPE html>
<html lang="ja">

<head>
<% const meta = {
  title: `${ title } | YouSync`,
  fileName: 'event'
}; %>
<%- include('components/_head.ejs', { meta }) %>
</head>

<body>

  <div class="wrap">
    
    <!-- ▼ Contents ▼ -->
    <div class="contents">

      <!-- ▼ entry ▼ -->
      <div id="entry" class="entry">
        <div class="entry_inner">
          <div class="entry_select">
            <span class="entry_select_title">入室</span>
          </div>
          <div class="entry_select">
            <span class="entry_select_title">管理</span>
          </div>
        </div>
      </div>

      <!-- ▼ Information ▼ -->
      <div id="information" class="information">
        <div class="information_inner">
          <h3 class="information_title"><%= title %></h3>
          <div class="information_contents">
            <p id="information_message"></p>
          </div>
        </div>
      </div>

    </div>
  </div>

  <!-- ▼ JavaScript ▼ -->
  <script src="/socket.io/socket.io.js"></script>
  <script>const getData = '<%- event %>'; const eventData = getData ? JSON.parse(getData) : null;</script>
  <script type="text/javascript" src="/js/event.js"></script>

</body>
</html>
